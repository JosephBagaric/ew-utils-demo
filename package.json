{
  "name": "ew-utils-demo",
  "version": "1.0.0",
  "description": "showcases the working of origin release B in a variety of situations.",
  "main": "dist/js/index.js",
  "types": "dist/js/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start-test-backend": "node node_modules/ew-utils-testbackend/index.js",
    "test": "npm run build-ts && node dist/js/test",
    "start-ganache": "ganache-cli -m 'chalk park staff buzz chair purchase wise oak receive avoid avoid home' -g 0 -l 80000000 -e 1000000 -a 20",
    "start": "npm run build-ts && node dist/js/test",
    "deploy-empty": "npm run build-ts && node dist/js/deployEmpty",
    "prebuild-ts": "rm -rf build/ts",
    "build-ts": "rm -rf dist/js && node_modules/typescript/bin/tsc",
    "prettier": "prettier --write --config-precedence file-override './src/**/*'",
    "lint": "./node_modules/.bin/tslint --fix  'src/**/*{.ts,.tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/energywebfoundation/ew-utils-demo.git"
  },
  "author": "slock.it GmbH; Martin Kuechler, martin.kuechler@slock.it; Heiko Burkhardt, heiko.burkhardt@slock.it; Chirag Parmar, chirag.parmar@slock.it",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/energywebfoundation/ew-utils-demo/issues"
  },
  "homepage": "https://github.com/energywebfoundation/ew-utils-demo#readme",
  "dependencies": {
    "ew-asset-registry-contracts": "0.0.99",
    "ew-asset-registry-lib": "0.0.99",
    "ew-erc-test-contracts": "0.0.99",
    "ew-market-contracts": "0.0.99",
    "ew-market-lib": "v0.3.1",
    "ew-origin-contracts": "0.0.99",
    "ew-origin-lib": "0.0.99",
    "ew-user-registry-contracts": "0.0.99",
    "ew-user-registry-lib": "0.0.99",
    "ew-utils-general-lib": "0.3.1",
    "ew-utils-testbackend": "v0.0.99",
    "web3": "1.0.0-beta.37",
    "winston": "3.2.1",
    "write-json-file": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "^9.6.41",
    "@types/web3": "1.0.14",
    "ganache-cli": "6.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-lines-between-class-members": "^1.3.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^2.9.2",
    "typescript-json-schema": "^0.31.0"
  },
  "postinstall": "rm -f node_modules/web3/index.d.ts",
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  }
}

{
  "name": "ew-demo",
  "version": "1.0.0",
  "description": "showcases the working of origin release B in a variety of situations.",
  "main": "dist/js/index.js",
  "types": "dist/js/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start-test-backend": "node node_modules/ew-utils-testbackend/index.js",
    "test": "npm run build-ts && node dist/js/market",
    "start-ganache": "ganache-cli -m 'chalk park staff buzz chair purchase wise oak receive avoid avoid home' -g 0 -l 80000000 -e 1000000 -a 20",
    "testlight": "npm run build-ts && mocha build/ts/test/",
    "debug-light-test": "npm run build-ts && mocha --inspect-brk build/ts/test/",
    "setup-SS": "npm run build-ts && node build/ts/utils/deployment/secretStore",
    "build-and-deploy": "npm run build && npm run deploy-contracts",
    "deploy-contracts": "node build/ts/utils/deployment/startMigrate",
    "build": "npm run compile-contracts && npm run build-ts && npm run build-schemas",
    "precompile-contracts": "rm -rf build/contracts",
    "compile-contracts": "node build/ts/utils/deployment/compile",
    "start-demo": "npm run build-ts && node dist/js/market",
    "deploy-empty": "npm run build-ts && node dist/js/deployEmpty",
    "prebuild-ts": "rm -rf build/ts",
    "build-ts": "rm -rf dist/js && node_modules/typescript/bin/tsc",
    "prebuild-schemas": "rm -rf dist/schemas && mkdir dist/schemas",
    "build-schemas": "npm run build-schema:AssetPropertiesOffChain && npm run build-schema:ProducingAssetPropertiesOffChain",
    "build-schema:AssetPropertiesOffChain": "typescript-json-schema --ignoreErrors --required src/blockchain-facade/Asset.ts OffChainProperties > dist/schemas/AssetPropertiesOffChain.schema.json",
    "build-schema:ProducingAssetPropertiesOffChain": "typescript-json-schema --ignoreErrors --required src/blockchain-facade/ProducingAsset.ts OffChainProperties > dist/schemas/ProducingAssetPropertiesOffChain.schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.slock.it/ewf/origin/asset-registry/ew-asset-registry-lib"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://git.slock.it/ewf/origin/asset-registry/ew-asset-registry-lib/issues"
  },
  "homepage": "https://git.slock.it/ewf/origin/asset-registry/ew-asset-registry-lib#readme",
  "dependencies": {
    "ew-user-registry-contracts": "0.1.0",
    "ew-asset-registry-contracts": "0.2.0",
    "ew-origin-contracts": "0.1.0",
    "ew-market-contracts": "0.1.1",
    "ew-user-registry-lib": "0.1.1",
    "ew-asset-registry-lib": "0.1.1",
    "ew-origin-lib": "0.1.1",
    "ew-market-lib": "0.1.0",
    "ew-utils-general-lib": "0.0.21",
    "ew-utils-testbackend": "0.0.8-release",
    "ew-erc-test-contracts": "0.0.2",
    "web3": "1.0.0-beta.37",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/node": "^9.6.41",
    "@types/web3": "1.0.14",
    "ganache-cli": "^6.3.0",
    "typescript": "^2.9.2",
    "typescript-json-schema": "^0.31.0"
  },
  "postinstall": "rm -f node_modules/web3/index.d.ts"
}

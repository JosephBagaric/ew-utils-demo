  "dependencies": {
    "ew-asset-registry-contracts": "0.2.0",
    "ew-asset-registry-lib": "0.1.1",
    "ew-erc-test-contracts": "0.0.2",
    "ew-market-contracts": "0.1.1",
    "ew-market-lib": "0.1.0",
    "ew-origin-contracts": "0.1.0",
    "ew-origin-lib": "0.1.1",
    "ew-user-registry-contracts": "0.1.0",
